cache=/work/ActIQ/product/engine/cache
tools=/work/ActIQ/product/engine/tools

y=$(ls ${cache}/*.classify.*.csv | sed -r 's:(.*)/(.*).classify.(...)-(...)(.*):\2,\3,\4:' | sort | uniq)

for x in $y
do
  v=$(echo $x|cut -d, -f1)
  s=$(echo $x|cut -d, -f2)
  e=$(echo $x|cut -d, -f3)

  process $v $s $e

done
