    eval "$(conda shell.bash hook)"
   conda activate sf
  export TF_CPP_MIN_LOG_LEVEL='2'
   clear

  video=${1-btTKApmxrtk}
  stime=${2-000}
  etime=${3-120}

  acquire    ${video}
  extract-mp ${video} ${stime} ${etime}
  predict-mp ${video} ${stime} ${etime}
  predict-sf ${video} ${stime} ${etime}
  predict-st ${video} ${stime} ${etime}
  combine.py ${video} ${stime} ${etime}
